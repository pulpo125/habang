   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "nums = [i for i in range(1, n+1)]\n",
    "queue = deque(nums)\n",
    "\n",
    "while len(queue) > 1:\n",
    "    queue.popleft()\n",
    "    queue.append(queue.popleft())\n",
    "\n",
    "print(queue[0])"
   ]
  }
